class Solution {

    int javaIntType(Scanner sc) {
        // code here
       int a=sc.nextInt();
        return a;
    }
    
    String javaStringType(Scanner sc) {
        // code here
       String str=sc.next();
        return str;
    }
    
    float javaFloatType(Scanner sc) {
        // code here
        float f=sc.nextFloat();
        return f;
    }
};
